#include <iostream>
using namespace std; 
#include <cmath>

#define PI 3.14

int main()
{
    float dt = 0.1; 
    float t[101];
    float y[101];
    
    for (int i=0; i<=100; i++) {
        t[i] = i*dt; 
        y[i] = sin(2*PI*t[i]);
        cout << "t=" << t[i] << ", y=" << y[i] << endl;   
    }
    
    cout << endl;
    
    int count = 0; 
    for (int i=0; i<=100; i++) {
        if (y[i] > 0) {
            cout << "y=" << y[i] << endl;
            count++; 
        }
    }
    cout << "Number of the counts = positive signal values: " << count << endl; 
    

    return 0;
}
