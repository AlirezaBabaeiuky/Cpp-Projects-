// now compare against the object over struct rather than class 
#include <iostream>
using namespace std; 

struct ST { 
    string name;
    char letter_grade;
    int grade;
    void fcn11() {
        cout << name << " " << letter_grade << " " << grade << endl;
    }
};

int main()
{
    ST student1;
    ST student2;
    ST student3;
    
    student1 = {"Allice", 'B', 84};
    student2 = {"Mathew", 'B', 81};
    student3 = {"Alex", 'A', 92}; 
    
    student1.fcn11();
    student2.fcn11();
    student3.fcn11();

    return 0;
}
