#include <iostream>
using namespace std; 

int main()
{
    int arr[] = {1, 3, 5, 7, 9, 0, 1, 0};
    for (int i=0; i<8; i++) { 
        cout << arr[i] << ' ';
    } 
    cout << "Counting number of zeros in the array:" << endl;
    int count = 0; 
    for (int i=0; i<8; i++) { 
        if (arr[i] == 0) { 
            count++;
        }
    }
    cout << "number of zeros in the array: " << count << endl; 

    return 0;
}
