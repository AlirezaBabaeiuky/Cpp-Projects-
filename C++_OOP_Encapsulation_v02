#include <iostream>
#include <string>
using namespace std;

class Car {
private:
    string Name;
    string OwnerName;
    int Year;
    int arr[3][2];

public:
    Car(string name, string ownername, int year, int array[3][2]) {
        Name = name;
        OwnerName = ownername;
        Year = year;
        for (int i = 0; i < 3; i++)
            for (int j = 0; j < 2; j++)
                arr[i][j] = array[i][j];
    }

    void GetInfo() {
        cout << "Car Name: " << Name << endl;
        cout << "Owner: " << OwnerName << endl;
        cout << "Year: " << Year << endl;
        cout << "Matrix:\n";
        for (int i = 0; i < 3; i++) {
            for (int j = 0; j < 2; j++)
                cout << arr[i][j] << " ";
            cout << endl;
        }
    }

    void SetInfo(string name, string ownername, int year, int array[3][2]) {
        Name = name;
        OwnerName = ownername;
        Year = year;
        for (int i = 0; i < 3; i++)
            for (int j = 0; j < 2; j++)
                arr[i][j] = array[i][j];
    }
};

int main() {
    int arr1[3][2] = {{1,2},{3,4},{5,6}};
    Car DealerCars("Lexus", "Alex", 2016, arr1);
    DealerCars.GetInfo();

    int arr2[3][2] = {{7,8},{9,10},{11,12}};
    DealerCars.SetInfo("Benz", "Alicia", 2025, arr2);
    DealerCars.GetInfo();

    return 0;
}
