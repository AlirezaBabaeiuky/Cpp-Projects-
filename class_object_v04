#include <iostream>
using namespace std;
class car {
    public:
    string brand;
    int year;
    void carinfo() {
        cout << "car brand is: " << brand << " and car year is: " << year << endl;
    }
}; 

int main()
{
    // make an object of the class: 
    car carDSP1;
    carDSP1.brand = "Lexus"; 
    carDSP1.year = 2025; 
    
    carDSP1.carinfo();

    return 0;
}
